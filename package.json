{
  "name": "carbyne-leaflet",
  "version": "0.1.0",
  "repository": "https://github.com/ceymard/carbyne-leaflet",
  "description": "some description",
  "license": "MIT",
  "main": "./src/index",
  "typings": "./index.d.ts",
  "scripts": {
    "build:dts": "dts-bundle --name carbyne-leaflet --main src/index.d.ts --out ../index.d.ts && sed -i '/\\(require\\|import\\).*\\.\\(styl\\|css\\)/d' ./index.d.ts",
    "build:js": "tsc",
    "build": "npm run build:js && npm run build:dts",
    "watch": "parallel -u ::: 'wr \"npm run build:dts\" src/*.d.ts' 'tscc -w'"
  },
  "dependencies": {
    "leaflet": "^0.7.7"
  },
  "devDependencies": {
    "@types/es6-collections": "0.0.28",
    "@types/es6-promise": "0.0.29",
    "@types/geojson": "0.0.28",
    "@types/leaflet": "^1.0.29"
  }
}
