{
  "name": "domic-leaflet",
  "version": "0.1.9",
  "repository": "https://github.com/ceymard/domic-leaflet",
  "description": "some description",
  "license": "MIT",
  "typings": "./index.d.ts",
  "scripts": {
    "build:dts": "dts-bundle --name domic-leaflet --main build/index.d.ts --out ../domic-leaflet.d.ts",
    "build:js": "tsc",
    "build": "npm run build:js && npm run build:dts && webpack",
    "watch": "parallel -u ::: 'ls build/*.d.ts | entr -d npm run build:dts' 'tscc -w' 'webpack -w'",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "leaflet": "^1.0.1"
  },
  "devDependencies": {
    "dts-bundle": "^0.6.1",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.13.3"
  }
}
