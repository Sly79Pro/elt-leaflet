{
  "name": "domic-leaflet",
  "version": "0.1.0",
  "repository": "https://github.com/ceymard/domic-leaflet",
  "description": "some description",
  "license": "MIT",
  "typings": "./index.d.ts",
  "scripts": {
    "build:dts": "dts-bundle --name domic-leaflet --main src/index.d.ts --out ../index.d.ts",
    "build:js": "tsc",
    "build": "npm run build:js && npm run build:dts",
    "watch": "parallel -u ::: 'ls src/*.d.ts | entr -d npm run build:dts' 'tscc -w'"
  },
  "dependencies": {
    "leaflet": "^1.0.1"
  },
  "devDependencies": {
    "@types/geojson": "0.0.28",
    "@types/leaflet": "^1.0.37",
    "dts-bundle": "^0.5.0",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.13.3"
  }
}
